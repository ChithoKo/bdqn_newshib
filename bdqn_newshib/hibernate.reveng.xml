<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>

	<table name="news_users" catalog="newssystem"
		class="com.bdqn.hib.pojo.User">
		<primary-key>
			<generator class="increment"></generator>
		</primary-key>
	</table>
	<table name="test" catalog="newssystem"
		class="com.bdqn.hib.pojo.Test">
		<primary-key>
			<generator class="increment"></generator>
		</primary-key>
	</table>
	<table name="comments" catalog="newssystem"
		class="com.bdqn.hib.pojo.Comments">
		<primary-key>
			<generator class="increment"></generator>
		</primary-key>
	</table>
	<table name="news" catalog="newssystem"
		class="com.bdqn.hib.pojo.News">
		<primary-key>
			<generator class="increment"></generator>
		</primary-key>
	</table>
<table name="topic" catalog="newssystem"
	class="com.bdqn.hib.pojo.Topic">
	<primary-key>
		<generator class="increment"></generator>
	</primary-key>
</table>
</hibernate-reverse-engineering>